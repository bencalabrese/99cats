<!DOCTYPE html>
<html>
<head>
  <title>NinetyNineCatsDay1</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

  <% if !current_user.nil? %>
    <p>Logged in as: <%= link_to current_user.user_name, user_url(current_user.id) %></p>
    <p><%= button_to "Logout", session_url, method: :delete %></p>
  <% else %>
    <%= button_to "Login", new_session_url, method: :get %>
  <% end %>

<%= yield %>

</body>
</html>
